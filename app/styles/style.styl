html, body {
	overflow: hidden;
	width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
}

#debuggerPanel {
	position: fixed;
	margin: auto;
	padding: 5px;
	background-color: white;
	opacity: 0.75;
	text-transform: uppercase;
	font-size: 0.75em;
}

#debuggerCommandList {
	padding: 0px;
}

.debuggerCommand {
	list-style-type: none;
	display: inline;
	text-transform: none;
}

#render {
	width: 100%;
	height: 100%;
	touch-action: none;
}
